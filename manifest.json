{
  "name": "nightServ",
  "description": "Ein Nightmode f√ºr IServ",
  "version": "1.0.0",
  "author" : "Adrian @ abit systems",
  "version_name": "beta",
  "manifest_version": 2,
  "web_accessible_resources": ["ab.png","whitetrans.png","nightservfull.svg","Nr.svg"],
  "permissions": [
    "storage",
    "tabs",
    "*://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "nightservN128.png"
  },
  "background": {
    "scripts": [
      "addon.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*","https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at" : "document_start"
    }
  ],
  "icons": {"128": "nightservN128.png","16" : "nightservN16.png", "19": "nightservN19.png", "38": "nightservN38.png", "48":"nightservN48.png"}
}